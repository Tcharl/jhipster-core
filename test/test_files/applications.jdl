application {
  config {
    baseName toto

    /* FreeStyle token wrapped in double quotes - (string literal) */
    path "foo/bar/blha.jdl.java"

    /* wrap a list of values in square parenthesis */
    languages [en, fr]

    /* optional commas */
    packageName com.mathieu.toto,
    enableTranslation false,
  }
}

application {
  config {
    baseName titi
    packageName com.mathieu.titi
    enableTranslation false
    languages [en, fr]
  }
}
